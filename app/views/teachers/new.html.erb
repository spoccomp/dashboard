<h1>Teacher#new</h1>

<p>Find me in app/views/teacher/create.html.erb</p>


<%= form_with scope: :teacher, model: @teacher, local: true do |form| %>
  <div>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </div>
 
  <div>
    <%= form.label :password %><br>
     <%= form.text_field :password%>
  </div>
 
 <div>
    <%= form.label :first_name %><br>
     <%= form.text_field :first_name %>
  </div>
  <div>
    <%= form.label :last_name %><br>
     <%= form.text_field :last_name %>
  </div>
  <div>
    <%= form.label :age %><br>
     <%= form.text_field :age %>
  </div>
  <div>
    <%= form.label :salary %><br>
     <%= form.text_field :salary%>
  </div>
  <div>
    <%= form.label :edu_level %><br>
     <%= form.text_field :edu_level %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>