<h1>List of Teachers</h1>
<% if @students.blank? %>
<p> There are no teachers currently in the system.</p>
<% else %>
<% @students.each do |student|%>
<tr>
    <td> <%= student.id%> </td>
    <td> <%= student.username%> </td>
    <td> <%= student.first_name%> </td>
    <td> <%= student.last_name%> </td>
    <td> <%= student.age%> </td>
    <td> <%= student.salary%> </td>
    <td> <%= student.edu_level%> </td>
    <td><%= link_to 'Edit', edit_student_path(student.id) %></td>
    <td><%= link_to 'Destroy', student_path(student.id),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td><br>
</tr>
<% end %>
<% end %>
<%= link_to 'Dashboard', admin_pages_path(@students) %>